<tryton>
    <data>
        <record model="ir.ui.view" id="product_barcode_view_tree">
            <field name="model">product.code</field>
            <field name="inherit" ref="product_barcode.product_barcode_view_tree"/>
            <field name="arch" type="xml">
                <![CDATA[
                <data>
                    <xpath
                        expr="/tree/field[@name=&quot;number&quot;]"
                        position="after">
                        <button string="Create label" name="label"/>
                        <field name="data"/>
                    </xpath>
                </data>
                ]]>
            </field>
        </record>

    </data>
</tryton>
